{
  "name": "code-challenge",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "rm priceboard.test.db && npm run test:seed && NODE_ENV=test jest --detectOpenHandles",
    "test:seed": "NODE_ENV=test npx knex migrate:latest && NODE_ENV=test npx knex seed:run",
    "start": "node index.js",
    "start:dev": "nodemon ./index.js",
    "d:build": "docker-compose build && docker-compose run --rm -e 'QUIET=1' code-challenge 'true'",
    "d:login": "docker-compose run --rm --service-ports code-challenge /bin/bash",
    "d:clean": "docker-compose stop && docker-compose rm -fv"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "knex": "^3.0.1",
    "mysql": "^2.18.1",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "supertest": "^6.3.4"
  }
}
