{
  "name": "code-challenge",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "dev": "NODE_ENV=development npm run update:db && NODE_ENV=development ts-node-dev --respawn --pretty --transpile-only ./src/server.ts",
    "docker:build": "docker build . -t app",
    "docker:run": "docker run -d -p 3000:3000 --name Api app",
    "start": "NODE_ENV=production npm run update:db && NODE_ENV=production node ./dist/src/server.js",
    "test": "make init-db-test && jest --coverage --detectOpenHandles",
    "update:db": "knex migrate:latest && knex seed:run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "knex": "^3.0.1",
    "sqlite3": "^5.1.6",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/parser": "^6.13.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
